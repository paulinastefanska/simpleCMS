extends layout

block content
  h1= title
  
  form(method='get')
    input(type='text' value=search name='search' class='input-search' placeholder='write...')
    input(type='submit' value='Search' class='btn-submit')

  each item in data
    article
      h1=item.title
      p(class='article-date')=item.date
      p=item.text